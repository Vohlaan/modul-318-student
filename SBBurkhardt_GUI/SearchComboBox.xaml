<UserControl x:Class="SBBurkhardt_GUI.SearchComboBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SBBurkhardt_GUI"
             mc:Ignorable="d" Height="20" Width="205.238">
    <Grid HorizontalAlignment="Left" Width="206" Height="20" VerticalAlignment="Top">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ComboBox x:Name="cboxSearch" TextBoxBase.TextChanged="cboxSearch_TextChanged" IsTextSearchEnabled="True" HorizontalAlignment="Left" VerticalAlignment="Top" Width="206" IsEditable="True" SelectionChanged="cboxSearch_SelectionChanged" Height="20"/>

    </Grid>
</UserControl>
